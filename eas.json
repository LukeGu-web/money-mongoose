{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "EXPO_PUBLIC_API_URL": "https://pi.lukegu.com/"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
